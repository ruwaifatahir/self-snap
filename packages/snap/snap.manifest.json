{
  "version": "0.1.0",
  "description": "Snap to resolve Self crypto names to addresses",
  "proposedName": "Self name resolution",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaMask/template-snap-monorepo.git"
  },
  "source": {
    "shasum": "tTWQxMvX+tANxxh32112BarP7wKRFuRYuk/gVWu08Nk=",
    "location": {
      "npm": {
        "filePath": "dist/bundle.js",
        "packageName": "snap",
        "registry": "https://registry.npmjs.org/"
      }
    }
  },
  "initialPermissions": {
    "snap_dialog": {},
    "endowment:rpc": {
      "dapps": true,
      "snaps": false
    },
    "endowment:name-lookup": {
      "chains": [
        "eip155:1",
        "eip155:56",
        "eip155:137",
        "eip155:43114",
        "eip155:42161"
      ],
      "matchers": {
        "tlds": [
          "self"
        ],
        "schemes": [
          "$"
        ]
      }
    },
    "endowment:network-access": {}
  },
  "manifestVersion": "0.1"
}
